<Window x:Class="lab4_prepCompany.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab4_prepCompany"
        mc:Ignorable="d"
        Title="Password Manager" Height="600" Width="1200"
        MinHeight="300" MinWidth="600" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
            <Button Name="signBtnMain" Width="100" Content="Sign In" Height="35" FontSize="20" Click="signBtnMain_Click"/>
            <Button x:Name="MainBtn" Visibility="Hidden" Click="MainBtn_Click"/>
        </StackPanel>
        <Frame Name="MainFrame" Margin="0,35,0,0">
            
        </Frame>
    </Grid>
</Window>
